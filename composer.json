{
    "name": "surazdott/laravel-repository-pattern",
    "description": "Laravel repository design pattern with service and interface.",
    "type": "package",
    "keywords": [
        "laravel",
        "repository-pattern",
        "service-container",
        "laravel-design-pattern",
        "laravel-repository-pattern"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Suraj Dott",
            "email": "surajdatheputhe@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "SurazDott\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SurazDott\\RepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
